<h1>Inside WJ's Bookshelf</h1>
<%= form_for @library do |f| %>
	
	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>
	
	<p>
		<%= f.label :author %><br>
		<%= f.text_area :author %>
	</p>
	
	<p>
		<%= f.label :rating %><br>
		<%= f.text_field :rating %>
	</p>

	<p>
		<%= f.label :review %><br>
		<%= f.text_field :review %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>
