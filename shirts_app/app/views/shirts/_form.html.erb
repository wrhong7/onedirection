<h1>New Shirts</h1>
<%= form_for @shirt do |f| %>
	
	<p>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</p>
	
	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</p>
	
	<p>
		<%= f.label :image %><br>
		<%= f.text_field :image %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>




